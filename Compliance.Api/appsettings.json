{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "CacheConnection": "{CacheName},abortConnect=false,ssl=true,allowAdmin=true,password={AccessKey}",
    "JWT": {
        "Authority": "#{GATEWAY-URL-OMNI-UAT}#/v1/issuer",
        "Audience": "ComplianceService"
    },
    "ConnectionStrings": {
        "DefaultConnection": "Server=tcp:{SQLServerName},1433;Initial Catalog=ComplianceStore;Persist Security Info=False;User ID={SQLServerUser};Password={SQLServerPassword};MultipleActiveResultSets=False;Encrypt=False;TrustServerCertificate=False;Connection Timeout=30;"
    },
  
    "SimSwapCheckTaskJobSetting": {
        "JobKey": "SimSwapCheckTaskJob",
        "IntervalMilliSeconds": 600000, //every 10 minutes
        "NextRunHour": 6,
        "MaximumAttemptCount": 8,
        "IsActive": true,
        "MaxIntervalMilliSeconds": 30000,
        "TaskQueueCount": 5,
        "MaxTaskCount": 100
    },
    "TelcosPrefix": {
